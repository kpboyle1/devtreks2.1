@using DevTreks.Helpers;
@model DevTreks.ViewModels.SearchViewModel

@{
    ViewBag.Title = AppHelper.GetResource("DEVTREKS_TITLE");
    ViewBag.Description = AppHelper.GetResource("DEVTREKS_GOAL");
    //Layout = "~/Views/Shared/_Layout";
    Layout = "_Layout";
}

@section Search
{
    @await Html.PartialAsync("_SearchBody", ViewData.Model)
}
@section Preview
{
    @await Html.PartialAsync("_ContentBody", ViewData.Model.SearchManagerData.SearchResult)
}
@section Select
{
    @await Html.PartialAsync("_ContentBody", ViewData.Model.SearchManagerData.SearchResult)
}
@section Edit
{
    @await Html.PartialAsync("_ContentBody", ViewData.Model.SearchManagerData.SearchResult)
}
@section Pack
{
    @await Html.PartialAsync("SubActionsViews/_Package", ViewData.Model.SearchManagerData.SearchResult)
}
@section LinkedView
{
    @await Html.PartialAsync("_ContentBody", ViewData.Model.SearchManagerData.SearchResult)
}
@section Member
{
    @await Html.PartialAsync("_Member", ViewData.Model.SearchManagerData.SearchResult)
}
@section UploadFile
{
  @HtmlHelperExtensions.FileUpload(Html, ViewData.Model.SearchManagerData.SearchResult)
}